<!DOCTYPE html>
<html>
<head>
  <title><%= yield(:title) %> | YukFutsal</title>
  <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <script src="//maps.google.com/maps/api/js?v=3.14&amp;sensor=false&amp;libraries=geometry" type="text/javascript"></script>
<script src="//google-maps-utility-library-v3.googlecode.com/svn/tags/markerclustererplus/2.1.2/src/markerclusterer_packed.js" type="text/javascript"></script>
  <%= csrf_meta_tags %>
</head>
<body class="<%= (dashboard? || dashboard_admin?) ? "dashboard" : (session? ? "session" : (list? ? "list" : "home")) %>">

<% if dashboard? || dashboard_admin? %>
	<%= render '/home/header' %>
	<div class="container">
	<div id="the-content">
	<%= render 'layouts/top' %>
	<div class="row">
		<div class="col-md-3">
			<% if logged_in?[1] == 'super' %>
				<%= render 'layouts/left_super' %>
			<% elsif logged_in?[1] == 'admin' %>
				<%= render 'layouts/left_admin' %>
			<% end %>
		</div>
		<div class="col-md-9">
			<%= yield %>
		</div>
	</div>
	</div>
<%= render "/home/footer" %>
	</div>
<% else %>
	<%= yield %>
<div class="scroll-top-wrapper ">
	<span class="scroll-top-inner">
		<i class="fa fa-2x fa-arrow-circle-up"></i>
	</span>
</div>
<% end %>

</body>
</html>
